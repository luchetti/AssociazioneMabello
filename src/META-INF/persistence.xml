<?xml version='1.0' encoding='UTF-8'?>
<!DOCTYPE xml>
<persistence version="2.1"
	xmlns="http://xmlns.jcp.org/xml/ns/persistence" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.w3.org/2001/XMLSchema-instance http://xmlns.jcp.org/xml/ns/persistence/persistence_2_1.xsd">
	<persistence-unit name="mabelloMysql"
		transaction-type="JTA">
		<jta-data-source>java:jboss/datasources/mabelloMysql</jta-data-source>
		<properties>
			<!-- Properties for Hibernate -->
			<property name="hibernate.show_sql" value="false" />
		</properties>
	</persistence-unit>
</persistence>